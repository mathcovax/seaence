{
  "name": "@service/marine-snow",
  "description": "Microservice de scrapping de données. Nommé d'après la neige marine, il récupère des données provenant de sources externes et les agrège pour les rendre accessibles aux autres services.",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch interfaces/main.ts",
    "test:lint": "eslint",
    "test:types": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "generate-types": "duplojs-types-codegen --import @duplojs/node/globals --include interfaces/routes/index.ts --output ../../libs/clients-type/types/horizon/duplojsTypesCodegen.d.ts"
  },
  "dependencies": {
    "@duplojs/core": "1.0.3",
    "@duplojs/node": "1.0.1",
    "@prisma/client": "^6.5.0",
    "dotenv": "16.4.5",
    "dotenv-expand": "12.0.1",
    "prisma": "^6.5.0"
  },
  "devDependencies": {
    "@duplojs/types-codegen": "1.0.1",
    "@types/node": "22.13.10",
    "tsx": "4.19.3",
    "typescript": "5.8.2"
  },
  "prisma": {
    "schema": "interfaces/providers/prisma/schema.prisma"
  }
}